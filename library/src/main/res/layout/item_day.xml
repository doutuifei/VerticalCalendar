<?xml version="1.0" encoding="utf-8"?>
<com.muzi.library.widget.MLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/viewDay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="10dp"
    android:gravity="center"
    android:orientation="vertical"
    tools:background="@color/bgSelected">

    <TextView
        android:id="@+id/textDay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/textNormal"
        android:textSize="@dimen/textDaySize"
        tools:text="9" />

    <TextView
        android:id="@+id/textState"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/marginDayState"
        android:textSize="@dimen/textStateSize"
        android:visibility="gone"
        tools:text="起租" />

</com.muzi.library.widget.MLinearLayout>